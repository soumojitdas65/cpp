#include <iostream>

int main() {
  
  int y = 0;

  std::cout << "Enter Year: \n";
  std::cin >> y;

  if (y<100 or y>9999){
    std::cout << "Invalid Entry.\n";
  }
  else if (y%4==0 or y%400==0){
    std::cout << "It is a leap year.\n";
  }
  else if (y%100==0 & y%400!=0){
    std::cout << "It is NOT a leap year.\n";
  }
  else{
    std::cout << "It is NOT a leap year.\n";
  }
}
